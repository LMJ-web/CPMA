<script>

    layui.use('element', function () {
        var element = layui.element;

    });

    //JQuery预加载
    $(function () {
        $(document).on("click", "[lay-id='']", function () {
            parent.location.reload();
        });
        
        if (sessionStorage.user != null && sessionStorage.user != "") {
            //重新转换为对象
            var User = JSON.parse(sessionStorage.admin);
            $("#uName").html(User.Nick);
            $("#Header").hide();
            $(".footer").hide();
            $("#xuanfu").remove();
            $("#Header").remove();
            $(".top").remove();
        }
    });

    //退出
    function Quit() {
        $.ajax({
            url: "/Admin/AdminLogOut",
            data: {},
            async: false,
            type: "post",
            success: function (data) {
                sessionStorage.removeItem("admin");
                layer.msg('退出成功！', {
                    time: 2000, //2s后自动关闭
                    btn: ['确认']
                });
                window.location.href = "/Home/Index"
            },
            error: function (data, type, err) {
                console.log("ajax错误类型：" + type);
                console.log(err);
            }
        });
    }

</script>